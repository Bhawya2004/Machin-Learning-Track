{
  "version": 2,
  "builds": [
    {
      "src": "./server/server.py",
      "use": "@vercel/python"
    },
    {
      "src": "./client/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/predict_home_price",
      "dest": "/server/server.py"
    },
    {
      "src": "/get_location_names",
      "dest": "/server/server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
